<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Spec.
    -->
    <changeSet id="20201223102615-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="lint_report_id"
                                 baseTableName="spec"
                                 constraintName="fk_spec_lint_report_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lint_report"/>

        <addForeignKeyConstraint baseColumnNames="portal_id"
                                 baseTableName="spec"
                                 constraintName="fk_spec_portal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="portal"/>

        <addForeignKeyConstraint baseColumnNames="capability_id"
                                 baseTableName="spec"
                                 constraintName="fk_spec_capability_id"
                                 referencedColumnNames="id"
                                 referencedTableName="capability"/>

        <addForeignKeyConstraint baseColumnNames="service_definition_id"
                                 baseTableName="spec"
                                 constraintName="fk_spec_service_definition_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service_definition"/>

        <addForeignKeyConstraint baseColumnNames="source_id"
                                 baseTableName="spec"
                                 constraintName="fk_spec_source_id"
                                 referencedColumnNames="id"
                                 referencedTableName="source"/>

    </changeSet>
</databaseChangeLog>
